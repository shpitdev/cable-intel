{
  "name": "cable-intel",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.3.1",
      "zod": "^4.3.6",
      "typescript": "^5.9.3",
      "convex": "^1.31.2"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build:vercel": "bash ./scripts/vercel-build.sh",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F @cable-intel/backend dev",
    "dev:setup": "turbo -F @cable-intel/backend dev:setup",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@cable-intel/env": "workspace:*",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@cable-intel/config": "workspace:*",
    "turbo": "^2.8.9",
    "typescript": "catalog:",
    "ultracite": "7.2.3"
  },
  "packageManager": "bun@1.3.8"
}
